{
  "description": "Remote Garage application for iOS/Android with Nativescript",
  "repository": "git@github.com:diestrin/remote-garage.git",
  "nativescript": {
    "id": "org.nativescript.remotegarage",
    "tns-ios": {
      "version": "2.2.0"
    },
    "tns-android": {
      "version": "2.2.0"
    }
  },
  "scripts": {
    "start-android": "tns run android --emulator",
    "start-ios": "tns run ios --emulator",
    "watch-android": "tns livesync android --emulator --watch",
    "watch-ios": "tns livesync ios --emulator --watch"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@ngrx/core": "^1.0.1",
    "@ngrx/effects": "^1.1.1",
    "@ngrx/store": "2.0.1",
    "@ngrx/store-devtools": "^2.0.0-beta.1",
    "@ngrx/store-log-monitor": "^2.0.0-beta.1",
    "nativescript-angular": "^0.3.0",
    "ngrx-store-router": "^0.4.0",
    "reflect-metadata": "^0.1.5",
    "tns-core-modules": "2.0.1"
  },
  "devDependencies": {
    "babel-traverse": "6.13.0",
    "babel-types": "6.13.0",
    "babylon": "6.8.4",
    "lazy": "1.0.11",
    "nativescript-dev-typescript": "^0.3.2",
    "nativescript-unit-test-runner": "0.3.3",
    "typescript": "^1.8.10",
    "codelyzer": "0.0.26",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-jasmine": "^1.0.2",
    "karma-nativescript-launcher": "^0.4.0",
    "tslint": "^3.14.0"
  }
}
